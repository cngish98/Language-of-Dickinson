<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>

<poem>
    <metadata>
        <first_line>Of Glory not a Beam is left</first_line>
        <date period="late">1886</date> 
        <poem_number>J1647</poem_number>
        <source>https://www.edickinson.org/editions/2/image_sets/12173041</source>
        <recipient>T. W. Higginson</recipient>
        <poem_themes></poem_themes>
    </metadata>
    
    <body>
        <stanza> 
            <line>Of Glory not a Beam is left</line>
            <line>But her Eternal House -</line>
            <line>The Asterisk is for the Dead,</line>
            <line>The Living, for the Stars -</line>
        </stanza>
    </body>
</poem>

