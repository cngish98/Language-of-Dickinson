<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>

<poem>
    <metadata>
        <first_line>The earth has many keys.</first_line>
        <date period="late">1885</date> 
        <poem_number>J1775</poem_number>
        <source>https://www.edickinson.org/editions/2/image_sets/12173302</source>
        <recipient>none</recipient>
        <poem_themes></poem_themes>
    </metadata>
    
    <body>
        <stanza>
            <line>The <nature nature_type="earth">earth</nature> has many keys.</line>
            <line>Where <music>melody</music> is not</line>
            <line>Is the unknown <place>peninsula</place>.</line>
            <line>beauty is <nature nature_type="general">nature's</nature> fact.</line>
        </stanza>

        <stanza>
            <line>But witness for her <nature nature_type="general">land</nature></line>
            <line>And witness for her <nature nature_type="water">sea</nature>,</line>
            <line>The <animal animal_type="cricket">cricket</animal> is her utmost</line>
            <line>Of elegy to me.</line>
        </stanza>
    </body>
</poem>

