<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>

<poem>
    <metadata>
        <first_line>Apparently with no surprise</first_line>
        <date period="late">1884</date> 
        <poem_number>J1624</poem_number>
        <source>https://www.edickinson.org/editions/2/image_sets/12172983</source>
        <recipient>none</recipient>
        <poem_themes></poem_themes>
    </metadata>
    
    <body>
        <stanza> 
            <line>Apparently with no surprise</line>
            <line>To any happy <nature nature_type="earth">Flower</nature></line>
            <line>The <nature nature_type="general">Frost</nature> beheads it at it's play -</line>
            <line>In accidental power -</line>
            <line>The blonde Assassin passes on -</line>
            <line>The <nature nature_type="space">Sun</nature> proceeds unmoved</line>
            <line>To measure off another <time>Day</time></line>
            <line>For an Approving <religion type="person">God</religion>.</line>
        </stanza>
    </body>
</poem>

