<poem>
   <metadata>
      <first_line>His Cheek is his Biographer -</first_line>
      <date period="late">1879</date>
      <poem_number>J1460</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12172552</source>
      <recipient>Edward (Ned) Dickinson</recipient>
      <poem_themes>body, religion</poem_themes>
   </metadata>
   <body>
      <stanza>
         <line>
            <physical_body>
               <token xml:id="t_1_1">His</token>
               <token xml:id="t_1_2">Cheek</token>
            </physical_body>
            <token xml:id="t_1_3">is</token>
            <token xml:id="t_1_4">his</token>
            <writing>
               <token xml:id="t_1_5">Biographer</token>
            </writing>
            <token xml:id="t_1_6">-</token>
         </line>
         <!-- not sure exactly what connections she is trying to make but I can glean the general themes. -->
         <line>
            <token xml:id="t_2_1">As</token>
            <token xml:id="t_2_2">long</token>
            <token xml:id="t_2_3">as</token>
            <token xml:id="t_2_4">he</token>
            <token xml:id="t_2_5">can</token>
            <physical_body>
               <token xml:id="t_2_6">blush</token>
            </physical_body>
         </line>
         <line>
            <religion type="thing">
               <token xml:id="t_3_1">Perdition</token>
            </religion>
            <token xml:id="t_3_2">is</token>
            <token xml:id="t_3_3">Opprobrium</token>
            <token xml:id="t_3_4">-</token>
         </line>
         <line>
            <token xml:id="t_4_1">Past</token>
            <token xml:id="t_4_2">that,</token>
            <token xml:id="t_4_3">he</token>
            <religion>
               <token xml:id="t_4_4">sins</token>
            </religion>
            <token xml:id="t_4_5">in</token>
            <emotion emotion_type="peaceful">
               <token xml:id="t_4_6">peace</token>
            </emotion>
            <token xml:id="t_4_7">-</token>
         </line>
      </stanza>
   </body>
</poem>
<!--POST PLAIN TEXT OF POEM BELOW -->
<!--     
    His Cheek is his Biographer -
    As long as he can blush
    Perdition is Opprobrium -
    Past that, he sins in peace - 
-->
