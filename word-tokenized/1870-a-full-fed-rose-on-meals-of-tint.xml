      <date period="middle">1870</date>
      <poem_number>J1154</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12171760</source>
      <recipient>Susan Dickinson</recipient>
      <poem_themes>nature, death, love</poem_themes>
      <ellipsis_present>yes</ellipsis_present>
   </metadata>
   <body>
      <stanza>
         <line>
            <token xml:id="t_1_1">A</token>
            <eating>
               <token xml:id="t_1_2">full</token>
               <token xml:id="t_1_3">fed</token>
            </eating>
            <nature nature_type="earth">
               <token xml:id="t_1_4">Rose</token>
            </nature>
            <token xml:id="t_1_5">on</token>
            <eating>
               <token xml:id="t_1_6">meals</token>
            </eating>
            <token xml:id="t_1_7">of</token>
            <token xml:id="t_1_8">Tint</token>
         </line>
         <line>
            <token xml:id="t_2_1">A</token>
            <eating>
               <token xml:id="t_2_2">Dinner</token>
            </eating>
            <token xml:id="t_2_3">for</token>
            <token xml:id="t_2_4">a</token>
            <animal animal_type="insect">
               <token xml:id="t_2_5">Bee</token>
            </animal>
         </line>
         <line>
            <token xml:id="t_3_1">In</token>
            <ellipsis ellipsis_type="nominal" elided_pos="determiner-article" possible_string="the"/>
            <token xml:id="t_3_2">process</token>
            <token xml:id="t_3_3">of</token>
            <token xml:id="t_3_4">the</token>
            <time>
               <token xml:id="t_3_5">Noon</token>
            </time>
            <token xml:id="t_3_6">became</token>
            <token xml:id="t_3_7">-</token>
         </line>
         <!--elision? -->
         <line>
            <token xml:id="t_4_1">Each</token>
            <light>
               <token xml:id="t_4_2">bright</token>
            </light>
            <death>
               <token xml:id="t_4_3">Mortality</token>
            </death>
         </line>
         <line>
            <token xml:id="t_5_1">The</token>
            <death>
               <token xml:id="t_5_2">Forfeit</token>
            </death>
            <token xml:id="t_5_3">is</token>
            <token xml:id="t_5_4">of</token>
            <token xml:id="t_5_5">Creature</token>
            <token xml:id="t_5_6">fair</token>
         </line>
