<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>
<poem>
   <metadata>
      <first_line>Summer for thee, grant I may be</first_line>
      <date period="early">1858</date>
      <poem_number>F6A</poem_number>
      <source> https://www.edickinson.org/editions/1/image_sets/12173327 </source>
      <recipient>none</recipient>
      <poem_themes>
         <theme>time</theme>
         <theme>nature</theme>
      </poem_themes>
      <ellipsis_present>yes</ellipsis_present>
   </metadata>
   <body>
      <stanza>
         <line>
            <time>
               <token xml:id="t_1_1">Summer</token>
            </time>
            <token xml:id="t_1_2">for</token>
            <token xml:id="t_1_3">thee,</token>
            <token xml:id="t_1_4">grant</token>
            <token xml:id="t_1_5">I</token>
            <token xml:id="t_1_6">may</token>
            <token xml:id="t_1_7">be</token>
         </line>
         <line>
            <token xml:id="t_2_1">When</token>
            <time>
               <token xml:id="t_2_2">Summer</token>
            </time>
            <time>
               <token xml:id="t_2_3">days</token>
            </time>
            <token xml:id="t_2_4">are</token>
            <token xml:id="t_2_5">flown!</token>
         </line>
         <line>
            <ellipsis ellipsis_type="clausal" possible_string="grant I may be"/>
            <token xml:id="t_3_1">Thy</token>
            <music>
               <token xml:id="t_3_2">music</token>
            </music>
            <token xml:id="t_3_3">still,</token>
            <token xml:id="t_3_4">when</token>
            <animal animal_type="bird">
               <token xml:id="t_3_5">Whippowil</token>
            </animal>
         </line>
         <line>
            <token xml:id="t_4_1">And</token>
            <animal animal_type="bird">
               <token xml:id="t_4_2">Oriole</token>
            </animal>
            <token xml:id="t_4_3">-</token>
            <token xml:id="t_4_4">are</token>
            <token xml:id="t_4_5">done!</token>
