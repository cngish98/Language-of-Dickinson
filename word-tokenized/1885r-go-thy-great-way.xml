<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>
<poem>
   <metadata>
      <first_line>Go thy great way!</first_line>
      <date period="late">1885</date>
      <poem_number>J1638</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12173019</source>
      <recipient>Benjamin Kimball</recipient>
      <poem_themes/>
   </metadata>
   <body>
      <stanza>
         <line>
            <token xml:id="t_1_1">Go</token>
            <token xml:id="t_1_2">thy</token>
            <token xml:id="t_1_3">great</token>
            <token xml:id="t_1_4">way!</token>
         </line>
         <line>
            <token xml:id="t_2_1">The</token>
            <nature nature_type="space">
               <token xml:id="t_2_2">Stars</token>
            </nature>
            <token xml:id="t_2_3">thou</token>
            <token xml:id="t_2_4">meetst</token>
         </line>
         <line>
            <token xml:id="t_3_1">Are</token>
            <token xml:id="t_3_2">even</token>
            <token xml:id="t_3_3">as</token>
            <token xml:id="t_3_4">Thyself</token>
            <token xml:id="t_3_5">-</token>
         </line>
         <line>
            <token xml:id="t_4_1">For</token>
            <token xml:id="t_4_2">what</token>
            <token xml:id="t_4_3">are</token>
            <nature nature_type="space">
               <token xml:id="t_4_4">Stars</token>
            </nature>
            <token xml:id="t_4_5">but</token>
            <token xml:id="t_4_6">Asterisks</token>
         </line>
         <line>
            <token xml:id="t_5_1">To</token>
            <token xml:id="t_5_2">point</token>
            <token xml:id="t_5_3">a</token>
            <token xml:id="t_5_4">human</token>
            <life>
               <token xml:id="t_5_5">Life</token>
            </life>
            <token xml:id="t_5_6">?</token>
         </line>
      </stanza>
   </body>
</poem>
<!--POST PLAIN TEXT OF POEM BELOW -->
<!--     
    Go thy great way!
    The Stars thou meetst
    Are even as Thyself -
    For what are Stars but Asterisks
    To point a human Life? 
-->
