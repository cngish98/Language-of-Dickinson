<poem>
   <metadata>
      <first_line>Witchcraft was hung, in History,</first_line>
      <date period="late">1883</date>
      <poem_number>J1583</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12172881</source>
      <recipient>none</recipient>
      <poem_themes/>
   </metadata>
   <body>
      <stanza>
         <line>
            <token xml:id="t_1_1">Witchcraft</token>
            <token xml:id="t_1_2">was</token>
            <token xml:id="t_1_3">hung,</token>
            <token xml:id="t_1_4">in</token>
            <memory>
               <token xml:id="t_1_5">History</token>
            </memory>
            <token xml:id="t_1_6">,</token>
         </line>
         <line>
            <token xml:id="t_2_1">But</token>
            <memory>
