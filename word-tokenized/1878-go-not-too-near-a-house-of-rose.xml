            </nature>
            <token xml:id="t_1_9">-</token>
         </line>
         <line>
            <token xml:id="t_2_1">The</token>
            <violence>
               <token xml:id="t_2_2">depredation</token>
            </violence>
            <token xml:id="t_2_3">of</token>
            <token xml:id="t_2_4">a</token>
            <nature nature_type="air">
               <token xml:id="t_2_5">Breeze</token>
