            </nature>
         </line>
         <line>
            <war>
               <token xml:id="t_4_1">Alarm</token>
               <token xml:id="t_4_2">it's</token>
               <token xml:id="t_4_3">walls</token>
               <token xml:id="t_4_4">away</token>
            </war>
            <token xml:id="t_4_5">-</token>
         </line>
         <!--marked up as war but idk if that's what she means -->
         <line>
            <token xml:id="t_5_1">Nor</token>
            <token xml:id="t_5_2">try</token>
            <token xml:id="t_5_3">to</token>
            <token xml:id="t_5_4">tie</token>
