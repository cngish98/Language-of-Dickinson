         </line>
         <line>
            <token xml:id="t_3_1">I</token>
            <token xml:id="t_3_2">think,</token>
            <token xml:id="t_3_3">because</token>
            <token xml:id="t_3_4">his</token>
            <eating>
               <token xml:id="t_3_5">empty</token>
               <token xml:id="t_3_6">Plate</token>
            </eating>
         </line>
         <line>
