<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>
<poem>
   <metadata>
      <first_line>The worthlessness of Earthly things</first_line>
      <date period="late">1876</date>
      <poem_number>J1373</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12172325</source>
      <recipient>none</recipient>
      <poem_themes>religion, nature</poem_themes>
      <ellipsis_present>yes</ellipsis_present>
   </metadata>
   <body>
      <stanza>
         <line>
            <token xml:id="t_1_1">The</token>
            <token xml:id="t_1_2">worthlessness</token>
            <token xml:id="t_1_3">of</token>
            <nature nature_type="general">
               <token xml:id="t_1_4">Earthly</token>
               <token xml:id="t_1_5">things</token>
            </nature>
         </line>
         <!--might this count as animals? plants? or just anything mortal ? humans too ? -->
         <line>
            <token xml:id="t_2_1">The</token>
            <music>
               <token xml:id="t_2_2">Ditty</token>
            </music>
            <token xml:id="t_2_3">is</token>
            <token xml:id="t_2_4">that</token>
            <nature nature_type="general">
               <token xml:id="t_2_5">Nature</token>
            </nature>
            <token xml:id="t_2_6">Sings</token>
            <token xml:id="t_2_7">-</token>
         </line>
         <line>
            <token xml:id="t_3_1">And</token>
            <token xml:id="t_3_2">then</token>
            <token xml:id="t_3_3">-</token>
            <ellipsis ellipsis_type="nominal"/>
            <token xml:id="t_3_4">enforces</token>
            <token xml:id="t_3_5">their</token>
            <emotion emotion_type="happiness">
               <token xml:id="t_3_6">delight</token>
            </emotion>
         </line>
         <!--elision here?-->
         <line>
            <token xml:id="t_4_1">Till</token>
            <religion type="thing">
               <token xml:id="t_4_2">Synods</token>
            </religion>
            <token xml:id="t_4_3">are</token>
            <token xml:id="t_4_4">inordinate</token>
            <token xml:id="t_4_5">-</token>
         </line>
      </stanza>
   </body>
</poem>
<!--POST PLAIN TEXT OF POEM BELOW -->
<!--     
    The worthlessness of Earthly things    (The ditty that nature sings is the worthlessness of earthly things)
    The Ditty is that Nature Sings -
    And then - enforces their delight
    Till Synods are inordinate - 
-->
