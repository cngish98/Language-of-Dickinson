<poem>
    <metadata>
        <title>The Poets light but Lamps -</title>
        <date>1864</date>
        <poem_number>J883</poem_number>
        <source>https://www.edickinson.org/editions/2/image_sets/12171126</source>
        <recipient>none</recipient>
    </metadata>
    
    <body>
        <stanza> 
            <line>The Poets light but Lamps -</line>
            <line>Themselves - go out -</line>
            <line>The Wicks they stimulate -</line>
            <line>If vital Light</line>
        </stanza>
        
        <stanza>
            <line>Inhere as do the Suns -</line>
            <line>Each Age a Lens</line>
            <line>Disseminating their</line>
            <line>Circumference -</line>
        </stanza>
    </body>
</poem>

