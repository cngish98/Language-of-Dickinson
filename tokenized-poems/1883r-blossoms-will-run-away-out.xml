<poem>
   <metadata>
      <first_line>Blossoms will run away,</first_line>
      <date period="late">1883</date>
      <poem_number>J1578</poem_number>
      <source>https://www.edickinson.org/editions/2/image_sets/12172867</source>
      <recipient>Cornelia (Nellie) Peck Sweetser</recipient>
      <poem_themes>
         <theme>nature</theme>
      </poem_themes>
      <ellipsis_present>yes</ellipsis_present>
   </metadata>
   <body>
      <stanza>
         <line>
            <nature nature_type="earth">
               <token xml:id="t_1_1">Blossoms</token>
            </nature>
            <token xml:id="t_1_2">will</token>
            <token xml:id="t_1_3">run</token>
            <token xml:id="t_1_4">away,</token>
         </line>
         <line>
            <token xml:id="t_2_1">Cakes</token>
            <ellipsis ellipsis_type="verbal"
                      elided_pos="modal-aux-verb"
                      possible_string="will"/>
            <token xml:id="t_2_2">reign</token>
            <token xml:id="t_2_3">but</token>
            <token xml:id="t_2_4">a</token>
            <time>
               <token xml:id="t_2_5">Day</token>
            </time>
            <token xml:id="t_2_6">,</token>
         </line>
         <line>
            <token xml:id="t_3_1">But</token>
            <memory>
               <token xml:id="t_3_2">Memory</token>
            </memory>
            <token xml:id="t_3_3">like</token>
            <music>
               <token xml:id="t_3_4">Melody</token>
            </music>
         </line>
         <line>
            <token xml:id="t_4_1">Is</token>
            <token xml:id="t_4_2">pink</token>
            <time>
               <token xml:id="t_4_3">Eternally</token>
            </time>
            <token xml:id="t_4_4">.</token>
         </line>
      </stanza>
   </body>
</poem>
<!--POST PLAIN TEXT OF POEM BELOW -->
<!--     
    Blossoms will run away,
    Cakes reign but a Day,
    But Memory like Melody
    Is pink Eternally. 
-->
