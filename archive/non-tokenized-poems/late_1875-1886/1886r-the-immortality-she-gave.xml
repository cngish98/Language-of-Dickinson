<?xml-model href="../dickinson_schema.rnc" type="application/relax-ng-compact-syntax"?>

<poem>
    <metadata>
        <first_line>The immortality she gave</first_line>
        <date period="late">1886</date> 
        <poem_number>J1648</poem_number>
        <source>https://www.edickinson.org/editions/2/image_sets/12173043</source>
        <recipient>T. W. Higginson</recipient>
        <poem_themes></poem_themes>
    </metadata>
    
    <body>
        <stanza> 
            <line>The <life>immortality</life> she gave</line>
            <line>We borrowed at her <death>Grave</death> -</line>
            <line>For just one Plaudit famishing,</line>
            <line>The Might of Human <love>love</love> -</line>
        </stanza>
    </body>
</poem>

